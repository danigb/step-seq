    "waa",
    "tempo"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/step-seq/issues"
  },
  "homepage": "https://github.com/danigb/step-seq#readme",
  "devDependencies": {
    "adsr": "^1.0.0",
    "soundfont-player": "^0.x.0",
    "tonal.note": "0.0.x",
    "vows": "^0.8.1"
  },
  "dependencies": {
    "curry": "^1.2.0"
  }
}
